#!/bin/bash
#PBS -N null2_72
#PBS -l nodes=1:ppn=10
#PBS -l walltime=5:00:00
#PBS -l pmem=16gb
#PBS -q compute
#PBS -m abe
#PBS -o null2_72.stdout
#PBS -e null2_72.stderr
#PBS -P RDS-FSC-VirusRates-RW


module load phyml
module load R
# Print the default PBS server
echo PBS default server is $PBS_DEFAULT
# Print the job's working directory and enter it.
echo Working directory is $PBS_O_WORKDIR
cd $PBS_O_WORKDIR
 
# Print some other environment information
echo Running on host `hostname`
echo Time is `date`
echo Directory is `pwd`
echo This jobs runs on the following processors:
NODES=`cat $PBS_NODEFILE`
echo $NODES
# Compute the number of processorsNPROCS=`wc -l < $PBS_NODEFILE`
echo This job has allocated $NPROCS nodes

Rscript /project/RDS-FSC-VirusRates-RW/genomeAdequacy/GenomeAdequacy/simulations/nulldataGTR/runGTR.R null_2.phy




